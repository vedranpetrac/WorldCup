<Window x:Class="WorldCupWPF.NationDetailsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorldCupWPF"
        mc:Ignorable="d"
        Title="NationDetailsWindow" Height="450" Width="800">
    <StackPanel>
        <Label>Naziv</Label>
        <Label x:Name="lblName"></Label>
        <Label>Fifa code</Label>
        <Label x:Name="lblCode"></Label>
        <Label>Broj odigranih/pobjeda/poraza/neodlučenih</Label>
        <Label x:Name="lblPlayed"></Label>
        <Label>Golova zabijenih/primljenih/razlika</Label>
        <Label x:Name="lblGoals"></Label>
    </StackPanel>
</Window>
